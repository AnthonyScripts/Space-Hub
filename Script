    local A_1 = "Stat"
    local A_2 = "Sword"
    game:GetService("ReplicatedStorage").Events.StatFunction:InvokeServer(A_1, A_2)
    end
    end)
    
    ChikaraTPToYou.Name = "ChikaraTPToYou"
    ChikaraTPToYou.Parent = Main
    ChikaraTPToYou.BackgroundColor3 = Color3.fromRGB(0, 85, 255)
    ChikaraTPToYou.BorderSizePixel = 0
    ChikaraTPToYou.Position = UDim2.new(0, 0, -0.00189876556, 0)
    ChikaraTPToYou.Size = UDim2.new(0, 230, 0, 46)
    ChikaraTPToYou.Font = Enum.Font.Cartoon
    ChikaraTPToYou.Text = "Chikara Crate TP To You"
    ChikaraTPToYou.TextColor3 = Color3.fromRGB(0, 0, 0)
    ChikaraTPToYou.TextSize = 23.000
    ChikaraTPToYou.TextWrapped = true
    ChikaraTPToYou.MouseButton1Click:connect(function()
    for i,v in pairs(game.Workspace.MouseIgnore:GetChildren())do
    if v.Name == "ChikaraCrate" then
    v:MoveTo(game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position)        
    end
    end
    wait(0.1) do
    game.StarterGui:SetCore("SendNotification", {
    Title = "Hello:)";
    Text = "Thanks for using Space Hub!";
    Duration = 2.0;
    })
    end
    end)
    
    Toggle.Name = "Toggle"
    Toggle.Parent = GUI
    Toggle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    Toggle.BackgroundTransparency = 1.000
    Toggle.Position = UDim2.new(0.405000001, 0, 0, 0)
    Toggle.Size = UDim2.new(0, 230, 0, 45)
    
    Open.Name = "Open"
    Open.Parent = Toggle
    Open.BackgroundColor3 = Color3.fromRGB(0, 85, 255)
    Open.BorderSizePixel = 0
    Open.Position = UDim2.new(1.86264515e-08, 0, 0, 0)
    Open.Size = UDim2.new(0, 115, 0, 45)
    Open.Font = Enum.Font.Cartoon
    Open.Text = "Open"
    Open.TextColor3 = Color3.fromRGB(0, 0, 0)
    Open.TextSize = 30.000
    Open.MouseButton1Click:connect(function()
        Close.Visible = true
        Open.Visible = false
        Main.Visible = true
    end)
    
    Close.Name = "Close"
    Close.Parent = Toggle
    Close.BackgroundColor3 = Color3.fromRGB(0, 85, 255)
    Close.BorderSizePixel = 0
    Close.Position = UDim2.new(0.5, 0, -0.0241211075, 0)
    Close.Size = UDim2.new(0, 115, 0, 46)
    Close.Visible = false
    Close.Font = Enum.Font.Cartoon
    Close.Text = "Close"
    Close.TextColor3 = Color3.fromRGB(0, 0, 0)
    Close.TextSize = 30.000
    Close.MouseButton1Click:connect(function()
        Close.Visible = false
        Open.Visible = true
        Main.Visible = false
    end)
    
    --Anti AFk
    
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
       vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
       wait(1)
       vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)

   
