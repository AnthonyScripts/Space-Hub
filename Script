    Swords.Text = "Sword"
    Swords.TextColor3 = Color3.fromRGB(0, 0, 0)
    Swords.TextSize = 30.000
    Swords.MouseButton1Down:connect(function()
    _G.LULU = true
    while _G.LULU do
